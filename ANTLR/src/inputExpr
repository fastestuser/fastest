\begin{zed}
[ncta] \\
saludos ::= hola | chau
\end{zed}

\begin{schemaType}{A}\\
a1, a2: ncta \\
a3: \num
\where
\end{schemaType}

\begin{schemaType}{B}\\
a1, a2: ncta \\
a3: A
\where
\end{schemaType}

\begin{schema}{Extraer}\\
f: saludos \pfun \seq saludos \\
x, y: B
\where
x.a3.a1 > 0
\end{schema}

